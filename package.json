{
  "name": "nestjs-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun --env-file=.env.dev nest start ",
    "start:dev": "bun start --watch",
    "start:debug": "bun start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "vitest run -c ./test/unit-test/config/vitest.unit.config.ts",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:e2e": "vitest run -c ./test/e2e/config/vitest.e2e.config.ts",
    "test:e2e-full": "bun db:test:reset && vitest run -c ./test/e2e/config/vitest.e2e.config.ts && bun db:rm test-db",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:dev:deploy": "prisma migrate deploy && prisma db seed",
    "db:rm": "docker compose rm  -s -f -v",
    "db:up": "docker compose up -d",
    "db:dev:reset": "bun db:rm dev-db && bun db:up dev-db && sleep 2 && bun --env-file=.env.dev prisma:dev:deploy",
    "db:test:reset": "bun db:rm test-db && bun db:up test-db && sleep 2 && bun --env-file=./test/e2e/config/.env.e2e.test prisma:dev:deploy && bun --env-file=./test/e2e/config/.env.e2e.test ./test/e2e/config/test-seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@prisma/client": "^5.20.0",
    "argon2": "^0.41.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@swc/core": "^1.7.26",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.1",
    "vitest-mock-extended": "^2.0.2"
  },
  "prisma": {
    "seed": "bun run prisma/seed/seed.ts"
  }
}
